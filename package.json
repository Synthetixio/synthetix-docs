{
	"name": "synthetix-docs",
	"version": "2.101.2",
	"description": "Synthetix docs builder",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "node scripts/build --overwrite && npm run format",
		"lint": "eslint .",
		"format": "prettier --write '**/*.js' '**/*.md' '**/*.yml'"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/Synthetixio/synthetix-docs.git"
	},
	"author": "Synthetix",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Synthetixio/synthetix-docs/issues"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && npm run format"
		}
	},
	"homepage": "https://github.com/Synthetixio/synthetix-docs#readme",
	"dependencies": {
		"axios": "~0.21.1",
		"marked": "^1.1.0",
		"moment": "~2.24.0",
		"numbro": "2.1.2",
		"synthetix": "2.101.2",
		"traverse": "^0.6.6",
		"yaml": "^1.10.0"
	},
	"devDependencies": {
		"chalk": "4.0.0",
		"eslint": "7.1.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-import": "2.20.2",
		"eslint-plugin-prettier": "3.1.3",
		"husky": "4.2.5",
		"prettier": "2.0.5"
	}
}
